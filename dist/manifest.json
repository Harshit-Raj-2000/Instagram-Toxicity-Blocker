{
    "name": "Instagram Toxicity blocker",
    "description": "block toxic messages",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons": {
        "16": "./icon-16.png",
        "32": "./icon-32.png", 
        "128": "./icon-128.png"
    },
    "background": {
        "scripts": ["./background.js"]
    },
    "browser_action":{

    },
    "permissions": [
        "https://www.instagram.com/*"
    ]
}